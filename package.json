{
  "name": "sqlectron-gui",
  "productName": "Sqlectron",
  "version": "1.2.0",
  "appBundleId": "sqlectron.app",
  "helperBundleId": "sqlectron.app.helper",
  "description": "A simple and lightweight SQL client with cross database and platform support",
  "author": "The Sqlectron Team",
  "main": "build/browser/main.js",
  "license": "MIT",
  "homepage": "https://sqlectron.github.io",
  "bugs": "https://github.com/sqlectron/sqlectron-gui/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/sqlectron/sqlectron-gui"
  },
  "scripts": {
    "dev:webpack": "webpack-dev-server --config ./webpack.dev.config.js",
    "dev:electron": "DEBUG=sqlectron-gui:*,sqlectron-core:*,spawn-auto-restart node scripts/dev-auto-restart.js",
    "test": "eslint .",
    "build": "NODE_ENV=production babel-node scripts/build.js --platform=all",
    "build:osx": "NODE_ENV=production babel-node scripts/build.js --platform=darwin",
    "build:linux": "NODE_ENV=production babel-node scripts/build.js --platform=linux",
    "build:win": "NODE_ENV=production babel-node scripts/build.js --platform=win32",
    "pack": "npm run pack:osx && npm run pack:win",
    "pack:osx": "electron-builder 'releases/Sqlectron-darwin-x64/Sqlectron.app' --platform=osx --out='installers/osx' --config=packager.json",
    "pack:win32": "electron-builder 'releases/Sqlectron-win32-ia32' --platform=win --out='installers/win/32' --config=packager.json",
    "pack:win64": "electron-builder 'releases/Sqlectron-win32-x64' --platform=win --out='installers/win/64' --config=packager.json",
    "build-pack": "npm run build && npm run pack",
    "build-pack:osx": "npm run build:osx && npm run pack:osx",
    "build-pack:win": "npm run build:win && npm run pack:win32 && npm run pack:win64",
    "build-pack-from-linux": "npm run build && npm run pack:win32 && npm run pack:win64",
    "publish": "./scripts/publish-releases.sh",
    "run-local-release": "babel-node scripts/run-local-release.js"
  },
  "keywords": [
    "electron",
    "sql",
    "client",
    "postgres",
    "mysql"
  ],
  "dependencies": {
    "axios": "^0.7.0",
    "babel": "^5.8.29",
    "babel-core": "^5.8.29",
    "babel-runtime": "^5.8.29",
    "csv-stringify": "0.0.8",
    "debug": "^2.2.0",
    "sqlectron-core": "^1.4.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "brace": "^0.7.0",
    "breakpoint-sass": "^2.6.1",
    "classnames": "^2.2.1",
    "css-loader": "^0.23.0",
    "del": "^2.2.0",
    "denodeify": "^1.2.1",
    "electron-builder": "^2.4.0",
    "electron-packager": "^5.1.1",
    "electron-prebuilt": "^0.36.0",
    "electron-release": "^2.2.0",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "file-loader": "^0.8.5",
    "history": "^1.12.6",
    "html-webpack-plugin": "^1.6.2",
    "imports-loader": "^0.6.5",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "minimist": "^1.2.0",
    "node-sass": "^3.4.2",
    "object-assign": "^4.0.1",
    "react": "^0.14.3",
    "react-ace": "^2.8.0",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.1",
    "react-resizable": "^1.0.1",
    "react-router": "^1.0.2",
    "react-select": "^0.9.1",
    "react-transform-hmr": "^1.0.1",
    "redux": "^3.0.5",
    "redux-logger": "^2.2.1",
    "redux-thunk": "^1.0.0",
    "sass-loader": "^3.1.2",
    "spawn-auto-restart": "^2.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "git+https://github.com/maxcnunes/webpack.git#v1.12.8-electron",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.1"
  }
}
